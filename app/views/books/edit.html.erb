 <h1>Editing Book</h1>

 <%= form_with model:@book, local:true do |f| %>
   <p>Title</p>
    <%= f.text_field :title %>
   <p>Body</p>
    <%= f.text_area :body %>

  <%= f.submit'Update Book' %>

 <a class="edit" href="/books">Edit</a> |
 <a class="back" href="/books">Back</a>